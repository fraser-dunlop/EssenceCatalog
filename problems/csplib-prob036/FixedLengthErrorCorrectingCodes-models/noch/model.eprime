language ESSENCE' 1.0

given Character_EnumSize: int
given maxDist: int(1..)
given codeWordLength: int(1..)
given numOfCodeWords: int(1..)
given fin1: int
given dist_FunctionAsRelation_RelationAsSet_Explicit_1_1: matrix indexed by [int(1..fin1)] of int(1..Character_EnumSize)
given dist_FunctionAsRelation_RelationAsSet_Explicit_1_2: matrix indexed by [int(1..fin1)] of int(1..Character_EnumSize)
given dist_FunctionAsRelation_RelationAsSet_Explicit_2: matrix indexed by [int(1..fin1)] of int(0..maxDist)
letting let1 be maxDist * codeWordLength
given minDist: int(0..let1)
find c_ExplicitR9_Function1D:
        matrix indexed by [int(1..numOfCodeWords), int(1..codeWordLength)] of int(1..Character_EnumSize)
such that
    and([or([!(c_ExplicitR9_Function1D[q5, q12] = c_ExplicitR9_Function1D[q4, q12]) | q12 : int(1..codeWordLength)]) \/
         or([!(c_ExplicitR9_Function1D[q4, q13] = c_ExplicitR9_Function1D[q5, q13]) | q13 : int(1..codeWordLength)])
         ->
         sum([sum([toInt(dist_FunctionAsRelation_RelationAsSet_Explicit_1_1[q7] = c_ExplicitR9_Function1D[q4, i] /\
                         dist_FunctionAsRelation_RelationAsSet_Explicit_1_2[q7] = c_ExplicitR9_Function1D[q5, i])
                   * catchUndef(dist_FunctionAsRelation_RelationAsSet_Explicit_2[q7], 0)
                       | q7 : int(1..fin1)])
                  | i : int(1..codeWordLength)])
         >= minDist
         /\
         and([or([dist_FunctionAsRelation_RelationAsSet_Explicit_1_1[q10] = c_ExplicitR9_Function1D[q4, i] /\
                  dist_FunctionAsRelation_RelationAsSet_Explicit_1_2[q10] = c_ExplicitR9_Function1D[q5, i]
                      | q10 : int(1..fin1)])
                  | i : int(1..codeWordLength)])
             | q4 : int(1..numOfCodeWords), q5 : int(1..numOfCodeWords)]),
    and([c_ExplicitR9_Function1D[q1, ..] <lex c_ExplicitR9_Function1D[q1 + 1, ..] | q1 : int(1..numOfCodeWords - 1)])

